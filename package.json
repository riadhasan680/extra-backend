{
  "name": "backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "build:vercel": "medusa build && rm -rf public && mkdir public && cp -r .medusa/server/public/admin/* public/",
    "link-inventory": "medusa exec ./src/scripts/link-inventory.ts",
    "check-setup": "medusa exec ./src/scripts/check-setup.ts",
    "seed-region": "medusa exec ./src/scripts/seed-region.ts",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start --host 0.0.0.0",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.13.1",
    "@medusajs/dashboard": "^2.13.1",
    "@medusajs/draft-order": "^2.13.1",
    "@medusajs/framework": "^2.13.1",
    "@medusajs/icons": "^2.13.1",
    "@medusajs/medusa": "^2.13.1",
    "@medusajs/ui": "^4.1.1",
    "@medusajs/utils": "^2.13.1",
    "@react-stately/datepicker": "^3.15.3",
    "@types/jsonwebtoken": "^9.0.10",
    "dodopayments": "2.15.0",
    "framer-motion": "^12.29.2",
    "inquirer": "^13.2.1",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.23",
    "motion": "^12.29.2",
    "pg": "^8.17.2",
    "rxjs": "^7.8.2",
    "xdg-basedir": "^5.1.0"
  },
  "devDependencies": {
    "@medusajs/cli": "^2.13.1",
    "@medusajs/test-utils": "^2.13.1",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.0",
    "vite": "^5.4.14"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@11.5.2"
}
